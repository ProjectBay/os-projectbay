{
  "name": "os-projectbay",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "commit": "cz",
    "lint": "npx nx lint",
    "format": "npx nx format:write",
    "pretty-format-check": "./tools/pretty-format-check.sh",
    "prepare": "husky install"
  },
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/projectbay/os-projectbay"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "keywords": [
    "angular",
    "angular material"
  ],
  "author": {
    "name": "Daniel Bersenkowitsch <daniel@projectbay.io>"
  },
  "engines": {
    "node": "^18.14.0",
    "pnpm": "^8.0.0"
  },
  "packageManager": "pnpm@8.12.1",
  "bugs": {
    "url": "https://github.com/projectbay/os-projectbay/issues"
  },
  "lint-staged": {
    "**/*.{ts,js,json,md,html,css,scss}": [
      "npx nx affected --target lint --uncommited --fix true",
      "npx nx affected --target lint --uncommited",
      "npx nx format:write --uncommited"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@angular/animations": "~16.2.0",
    "@angular/cdk": "^16.2.6",
    "@angular/common": "~16.2.0",
    "@angular/compiler": "~16.2.0",
    "@angular/core": "~16.2.0",
    "@angular/forms": "~16.2.0",
    "@angular/material": "^16.2.6",
    "@angular/platform-browser": "~16.2.0",
    "@angular/platform-browser-dynamic": "~16.2.0",
    "@angular/router": "~16.2.0",
    "@nx/angular": "16.9.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~16.2.0",
    "@angular-devkit/core": "~16.2.0",
    "@angular-devkit/schematics": "~16.2.0",
    "@angular-eslint/eslint-plugin": "~16.0.0",
    "@angular-eslint/eslint-plugin-template": "~16.0.0",
    "@angular-eslint/template-parser": "~16.0.0",
    "@angular/cli": "~16.2.0",
    "@angular/compiler-cli": "~16.2.0",
    "@angular/language-service": "~16.2.0",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@commitlint/config-nx-scopes": "^18.4.3",
    "@commitlint/cz-commitlint": "^18.4.3",
    "@nx/cypress": "16.9.1",
    "@nx/eslint-plugin": "16.9.1",
    "@nx/jest": "16.9.1",
    "@nx/js": "16.9.1",
    "@nx/linter": "16.9.1",
    "@nx/web": "16.9.1",
    "@nx/workspace": "16.9.1",
    "@schematics/angular": "~16.2.0",
    "@types/jest": "^29.4.0",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "commitizen": "^4.3.0",
    "cypress": "^13.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "~8.46.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.13.4",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-preset-angular": "~13.1.0",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "^15.2.0",
    "ng-packagr": "~16.2.0",
    "nx": "16.9.1",
    "postcss": "^8.4.5",
    "postcss-import": "~14.1.0",
    "postcss-preset-env": "~7.5.0",
    "postcss-url": "~10.1.3",
    "prettier": "^2.6.2",
    "semantic-release-npm-github-publish": "^1.5.5",
    "semantic-release-plus": "^20.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.1.3"
  }
}
